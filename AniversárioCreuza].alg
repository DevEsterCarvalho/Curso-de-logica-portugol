algoritmo "AniversárioCreuza"

var
     anoNasc, anoAtual, idade: inteiro
inicio
      Escreva ("em que ano nós estamos? ")
      leia (anoAtual)
      Escreva ("em que ano você nasceu? ")
      Leia (anoNasc)
      Idade <- anoAtual - anoNasc
      Escreva ("minha idade é ",idade, " anos")
      

fimalgoritmo
