algoritmo "ÁreaTrápezio"

var
   a,b,c, area: real

inicio
      Escreval ("Escreva o valor da base maior: ")
      Leia (A)
      Escreval ("Escreva o valor da base menor: ")
      Leia (b)
      Escreval ("Escreva o valor da altura : ")
      Leia (c)
      area <- (a+b)/2*c
      Escreva ("A área do Trapézio é: ", area)
      
fimalgoritmo
