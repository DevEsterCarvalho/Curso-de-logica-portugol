algoritmo "Volume"
// Função :
// Autor :
// Data : 02/03/2023
// Seção de Declarações 
var
   raio, altura, volume: real
inicio
      Escreva ("Escreva o valor do raio: ")
      Leia (raio)
      Escreva ("Escreval o valor da altura: ")
      Leia (altura)
      volume <- PI* raio^2*altura
      Escreva ("o volume da caixa dágua é: ", volume)
fimalgoritmo
