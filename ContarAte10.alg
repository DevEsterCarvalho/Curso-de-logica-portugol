algoritmo "ContarAte10"
// Função :
// Autor :
// Data : 16/02/2023
// Seção de Declarações 
var
   contador: inteiro
inicio
      contador <- 0
      EnquantO (contador <= 10) faca
      Escreval (contador)
      contador <- contador + 1
      FimEnquanto
      Escreval ("Terminei de contar")
fimalgoritmo
